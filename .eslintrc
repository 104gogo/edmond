{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": 0,
    "import/no-extraneous-dependencies": 0,
    "react/react-in-jsx-scope": 0,
    "react/prop-types": 0,
    "no-unused-expressions": 0,
    "import/prefer-default-export": 0
  },
  "globals": {
    "document": true,
    "React": true,
    "window": true,
    "fetch": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "legacyDecorators": true
    }
  }
}
